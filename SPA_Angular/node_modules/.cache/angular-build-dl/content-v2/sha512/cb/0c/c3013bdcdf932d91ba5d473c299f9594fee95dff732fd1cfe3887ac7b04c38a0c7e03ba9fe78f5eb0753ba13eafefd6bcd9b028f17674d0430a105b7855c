ateCustomValue(e,t,i,s,n,r,o){return this.findLocation(e,t,i,s,n)}getHorizontalDirection(e,t,i,s,n,r){let o="",a="";return o+="M "+e.x+" "+t.y+" L "+i.x+" "+t.y,a+="Plus"===s||"Both"===s?"M "+e.x+" "+(t.y-r)+" L "+e.x+" "+(t.y+r):"",a+="Minus"===s||"Both"===s?"M "+i.x+" "+(t.y-r)+" L "+i.x+" "+(t.y+r):" ",[o,a]}getVerticalDirection(e,t,i,s,n,r){let o="",a="";return o+="M "+t.x+" "+e.y+" L "+t.x+" "+i.y,a+="Plus"===s||"Both"===s?"M "+(t.x-r)+" "+e.y+" L "+(t.x+r)+" "+e.y:"",a+="Minus"===s||"Both"===s?"M "+(t.x-r)+" "+i.y+" L "+(t.x+r)+" "+i.y:"",[o,a]}getBothDirection(e,t,i,s,n,r){let o="",a="";const l=this.getHorizontalDirection(e,t,i,s,n,r),h=this.getVerticalDirection(e,t,i,s,n,r);return a=l[0].concat(h[0]),o=l[1].concat(h[1]),[a,o]}getErrorDirection(e,t,i,s,n){const r=s.errorBar.direction,o=s.errorBar.mode,a=s.errorBar.errorBarCap.length;let l,h=o;switch(o){case"Both":h=o;break;case"Horizontal":h=n?"Vertical":o;break;case"Vertical":h=n?"Horizontal":o}switch(h){case"Horizontal":l=this.getHorizontalDirection(e,t,i,r,h,a);break;case"Vertical":l=this.getVerticalDirection(e,t,i,r,h,a);break;case"Both":l=this.getBothDirection(e,t,i,r,h,a)}return[l[0],l[1]]}meanCalculation(e,t){let i=0,s=0,n=0,r=0;const o=e.points.length;switch(t){case"Vertical":s=Kp(e.yData),n=s/o;break;case"Horizontal":i=Kp(e.xData),r=i/o;break;case"Both":s=Kp(e.yData),n=s/o,i=Kp(e.xData),r=i/o}for(const d of e.points)"Vertical"===t?s+=Math.pow(d.yValue-n,2):("Horizontal"===t||(s+=Math.pow(d.yValue-n,2)),i+=Math.pow(d.xValue-r,2));const a=s/(o-1),l=Math.sqrt(s/(o-1)),h=i/(o-1),c=Math.sqrt(i/(o-1));return new rg(a,l,h,c,n,r)}createElement(e,t){const i="Cartesian"===t.chartAreaType?"translate("+e.clipRect.x+","+e.clipRect.y+")":"",s=e.marker.height/2,n=e.marker.width/2;if("Cartesian"===t.chartAreaType){const r=new sm(t.element.id+"_ChartErrorBarClipRect_"+e.index,"transparent",{width:1,color:"Gray"},1,{x:-n,y:-s,width:e.clipRect.width+2*n,height:e.clipRect.height+2*s});e.errorBarElement=t.renderer.create