ired],listPart:[[]],createdBy:[0],productionDate:[new Date]})}ngOnInit(){this.excelDownloadUrl=`${a.a.apiUrlEC}schedule/ExcelExport`,this.toolbar=["Import Excel","Export Excel","Search","Add"],this.filterSettings={type:"Excel"},this.editSettings={allowEditing:!1,allowAdding:!0,allowDeleting:!1,newRowPosition:"Normal"},this.getAllUsers(),this.getAllTreatment()}onClickDefault(){this.systemDate=null,this.getAll()}onChangeDate(e){this.scheduleDate=this.datePipe.transform(e.value,"yyyy-MM-dd")}onChangeSystemDate(e){this.systemDate=e.value,null!==this.systemDate&&this.getAllByDate()}search(e){this.gridObj.search(this.name)}getAllByDate(){this.spinner.show(),setTimeout(()=>{this.scheduleService.getAllWithDate(this.systemDate.toDateString()).subscribe(e=>{this.data=e.map(e=>({id:e.id,approvalStatus:!0===e.approvalStatus?"Yes":"No",articleNo:e.articleNo,establishDate:this.datePipe.transform(e.establishDate,"yyyy-MM-dd"),productionDate:this.datePipe.transform(e.productionDate,"yyyy-MM-dd"),artProcess:e.artProcess,finishedStatus:!0===e.finishedStatus?"Yes":"No",modelNo:e.modelNo,modelName:e.modelName,season:e.season,treatment:e.treatment,process:e.process,parts:e.parts,approvalBy:e.approvalBy,createdBy:e.createdBy,processID:e.processID,color:e.color})),this.dataTamp=this.data,this.spinner.hide()})},1e3)}getAllTreatment(){this.treatmentWayService.getAllProcess().subscribe(e=>{this.treatmentData=e})}dataBound(){this.flag=!0}filterTreatment(e){this.flag&&("Print"===e.name?(this.gridObj.hideColumns("Process"),this.flag=!0):this.gridObj.showColumns("Process"),this.data=this.dataTamp.filter(t=>t.treatment===e.name))}filterDefault(){this.data=this.dataTamp}onClickToolbar(e){const t=e.target.closest("button");switch(null==t?void 0:t.id){case"Import Excel":this.showModal(this.importModal);break;case"Add":e.cancel=!0,this.openAddModal(this.AddModal);break;case"Export Excel":this.gridObj.columns[0].visible=!1,this.gridObj.columns[10].visible=!1,this.gridObj.excelExport({fileName:"Schedul